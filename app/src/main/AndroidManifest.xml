<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.james.godlike">

    <application
        android:name="com.godlike.JamesApplication"
        android:allowBackup="true"
        android:icon="${app_icon}"
        android:label="${app_name}"
        android:roundIcon="${app_icon}"
        android:supportsRtl="true"
        android:theme="@style/Theme.GodLike">
        <activity android:name="com.godlike.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="eventbus.EventPrcActivity" />
        <activity android:name="eventbus.EventPrcActivity2" />
        <activity android:name="com.godlike.arouter.ARouterActivity" />
    </application>

</manifest>